{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "no-plusplus": "off",
    "react/react-in-jsx-scope": "off",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}
